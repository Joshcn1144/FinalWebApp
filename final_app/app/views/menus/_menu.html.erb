<div id="<%= dom_id menu %>">
  <p>
    <strong>Name:</strong>
    <%= menu.name %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= menu.description %>
  </p>

  <p>
    <strong>Price:</strong>
    <%= menu.price %>
  </p>

  <p>
    <strong>Available:</strong>
    <%= menu.available %>
  </p>

  <p>
    <strong>User:</strong>
    <%= menu.user_id %>
  </p>

</div>
